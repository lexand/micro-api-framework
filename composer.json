{
  "name": "lexand/micro-api-framework",
  "description": "API (or single page application) oriented micro framework",
  "version": "0.0.16-alpha",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Olexandr Dorogyh",
      "email": "dorogikh.alexander@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "microapi\\": [
        "src/microapi/",
        "tests/units/microapi/"
      ],
      "tests\\utils\\": "tests/utils"
    },
    "files": [
      "src/microapi/functions_include.php"
    ]
  },
  "require": {
    "php": ">=7.0",
    "psr/http-message": "1.*",
    "guzzlehttp/psr7": "1.*"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "6.*",
    "codeception/codeception": "2.*",
    "phpunit/phpunit": "6.*"
  },
  "scripts": {
    "test": "phpunit"
  }
}
